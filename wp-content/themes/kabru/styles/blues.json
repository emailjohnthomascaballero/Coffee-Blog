		"color": {
			"palette": [
				{
					"color": "#4B6CB7",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#c8d6ef",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#0b417a",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
                    "color": "#f0f5fb",
                    "name": "Default background",
                    "slug": "default"
                }
			]
